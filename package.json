{
    "name": "gatsby-starter-lemonade",
    "private": true,
    "description": "Gatsby Starter Lemonade",
    "version": "2.3.1",
    "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
    "keywords": [
        "gatsby",
        "stylus",
        "eslint",
        "seo",
        "code formatter",
        "mk-react-comp",
        "pwa",
        "progressive web app",
        "github actions",
        "lemonade",
        "gatsby-starter",
        "semantic release",
        "pre commit"
    ],
    "license": "MIT",
    "scripts": {
        "commit": "git-cz",
        "setup": "node setup/setupMessage.mjs && yarn && node setup/setup.mjs && yarn start",
        "setup:copy": "rimraf setupCopy && cp ./package.json ./setup/package.json && cp -r ./setup ./setupCopy",
        "setup:test": "yarn setup:copy && node setup/setupMessage.mjs && yarn && yarn setup:test:init && yarn start",
        "setup:test:init": "cross-env MODE=test node setup/setup.mjs",
        "develop": "GATSBY_ACTIVE_ENV=development gatsby develop -o",
        "start": "yarn develop",
        "build": "GATSBY_ACTIVE_ENV=production gatsby build",
        "build:staging": "GATSBY_ACTIVE_ENV=staging gatsby build",
        "start:mobile": "yarn develop -H 0.0.0.0",
        "serve": "gatsby serve -o",
        "serveMobile": "gatsby serve -o -H 0.0.0.0",
        "optim-img": "node scripts/imageOptimizer.js",
        "https": "http-server public -o -g --ssl --cert ~/.localhost-ssl/test.crt --key ~/.localhost-ssl/test.key",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "stylint": "stylint",
        "lint": "eslint --ext .jsx,.js,.cjs,.mjs .",
        "pretty": "prettier --write 'src/**/*.js'",
        "clean": "gatsby clean",
        "mrc": "node ./node_modules/mk-react-comp/lib/index.js",
        "semantic-release": "semantic-release",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JimmyBeldone/gatsby-starter-lemonade"
    },
    "bugs": {
        "url": "https://github.com/JimmyBeldone/gatsby-starter-lemonade/issues"
    },
    "dependencies": {
        "@reach/router": "1.3.4",
        "@reduxjs/toolkit": "1.9.7",
        "babel-plugin-styled-components": "2.0.7",
        "clsx": "1.2.1",
        "dotenv": "16.0.3",
        "gatsby": "4.24.4",
        "gatsby-plugin-alias-imports": "1.0.5",
        "gatsby-plugin-breakpoints": "1.3.9",
        "gatsby-plugin-force-trailing-slashes": "1.0.6",
        "gatsby-plugin-image": "2.24.0",
        "gatsby-plugin-manifest": "4.24.0",
        "gatsby-plugin-netlify": "5.0.1",
        "gatsby-plugin-offline": "5.24.0",
        "gatsby-plugin-react-helmet-async": "1.2.3",
        "gatsby-plugin-robots-txt": "1.7.1",
        "gatsby-plugin-sharp": "4.24.0",
        "gatsby-plugin-sitemap": "5.24.0",
        "gatsby-plugin-styled-components": "5.24.0",
        "gatsby-plugin-typography": "4.24.0",
        "gatsby-source-filesystem": "4.24.0",
        "gatsby-transformer-sharp": "4.24.0",
        "intersection-observer": "0.12.2",
        "lodash.kebabcase": "4.1.1",
        "path-browserify": "1.0.1",
        "process": "0.11.10",
        "prop-types": "15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-headroom": "3.2.1",
        "react-helmet-async": "1.3.0",
        "react-redux": "8.0.4",
        "react-typography": "0.16.20",
        "reactotron-core-client": "2.8.10",
        "redux": "4.2.0",
        "redux-persist": "6.0.0",
        "styled-components": "5.3.6",
        "typeface-lato": "1.1.13",
        "typeface-montserrat": "1.1.13",
        "typography": "0.16.21",
        "typography-theme-moraga": "0.16.19"
    },
    "devDependencies": {
        "@babel/core": "7.19.3",
        "@babel/eslint-parser": "7.19.1",
        "@babel/preset-env": "7.19.4",
        "@babel/preset-react": "7.18.6",
        "@semantic-release/changelog": "6.0.1",
        "@semantic-release/git": "10.0.1",
        "babel-eslint": "10.1.0",
        "babel-preset-gatsby": "2.24.0",
        "commitizen": "4.2.5",
        "cross-env": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "devmoji": "2.3.0",
        "eslint": "8.25.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.10",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-sort-destructure-keys": "1.4.0",
        "eslint-plugin-sort-keys-fix": "1.1.2",
        "eslint-webpack-plugin": "3.2.0",
        "gatsby-plugin-eslint": "4.0.2",
        "gatsby-plugin-stylus-resources": "2.0.5",
        "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.30",
        "husky": "8.0.1",
        "lint-staged": "13.0.3",
        "prettier": "2.7.1",
        "reactotron-react-js": "3.3.8",
        "reactotron-redux": "3.1.3",
        "replace": "1.2.1",
        "rimraf": "3.0.2",
        "semantic-release": "19.0.5",
        "stylint": "2.0.0"
    },
    "lint-staged": {
        "{src}/**/*.(js|jsx)": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.(styl|css)": [
            "stylint"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
