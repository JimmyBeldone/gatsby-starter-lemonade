{
  "name": "gatsby-starter-lemonade",
  "private": true,
  "description": "Gatsby Starter Lemonade",
  "version": "1.0.12",
  "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
  "keywords": [
    "gatsby",
    "stylus",
    "eslint",
    "seo",
    "code formatter",
    "mk-react-comp",
    "pwa",
    "progressive web app",
    "lemonade",
    "gatsby-theme"
  ],
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "setup": "node setup/setupMessage.js && yarn && node setup/setup.js",
    "develop": "cross-env NODE_ENV=development MODE=development gatsby develop -o",
    "start": "yarn develop",
    "build": "cross-env NODE_ENV=production MODE=production gatsby build",
    "build:staging": "cross-env NODE_ENV=production MODE=staging gatsby build",
    "start:mobile": "yarn develop -H 0.0.0.0",
    "serve": "gatsby serve -o",
    "serveMobile": "gatsby serve -o -H 0.0.0.0",
    "optim-img": "node scripts/imageOptimizer.js",
    "https": "http-server public -o -g --ssl --cert ~/.localhost-ssl/test.crt --key ~/.localhost-ssl/test.key",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "stylint": "stylint",
    "lint": "eslint .",
    "pretty": "prettier --write 'src/**/*.js'",
    "clean": "rimraf .cache public",
    "mrc": "node ./node_modules/mk-react-comp/lib/index.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyBeldone/gatsby-starter-lemonade"
  },
  "bugs": {
    "url": "https://github.com/JimmyBeldone/gatsby-starter-lemonade/issues"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.11.1",
    "clsx": "1.1.1",
    "gatsby": "2.24.63",
    "gatsby-image": "2.4.19",
    "gatsby-plugin-breakpoints": "1.2.2",
    "gatsby-plugin-manifest": "2.4.30",
    "gatsby-plugin-netlify": "2.3.15",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-nprogress": "2.3.11",
    "gatsby-plugin-offline": "3.2.28",
    "gatsby-plugin-react-helmet-async": "1.1.0",
    "gatsby-plugin-robots-txt": "1.5.2",
    "gatsby-plugin-sharp": "2.6.36",
    "gatsby-plugin-sitemap": "2.4.13",
    "gatsby-plugin-styled-components": "3.3.11",
    "gatsby-plugin-typography": "2.5.11",
    "gatsby-schema-field-absolute-path": "1.2.2",
    "gatsby-source-filesystem": "2.3.30",
    "gatsby-transformer-sharp": "2.5.15",
    "grommet": "2.15.0",
    "intersection-observer": "0.11.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-headroom": "3.0.0",
    "react-helmet-async": "1.0.7",
    "react-share": "4.2.1",
    "react-typography": "0.16.19",
    "styled-components": "5.2.0",
    "typeface-lato": "0.0.75",
    "typeface-montserrat": "0.0.75",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "babel-eslint": "10.1.0",
    "chalk": "4.1.0",
    "commitizen": "4.2.1",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.9.0",
    "eslint-config-universe": "4.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-stylus-resources": "1.0.29",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.12",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "prompt": "1.0.0",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "semantic-release": "17.1.2",
    "stylint": "2.0.0",
    "typescript": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.(js|jsx)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.(styl|css)": [
      "stylint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
