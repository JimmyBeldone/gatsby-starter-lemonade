{
    "name": "gatsby-starter-lemonade",
    "private": true,
    "description": "Gatsby Starter Lemonade",
    "version": "2.3.1",
    "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
    "keywords": [
        "gatsby",
        "stylus",
        "eslint",
        "seo",
        "code formatter",
        "mk-react-comp",
        "pwa",
        "progressive web app",
        "github actions",
        "lemonade",
        "gatsby-starter",
        "semantic release",
        "pre commit"
    ],
    "license": "MIT",
    "scripts": {
        "commit": "git-cz",
        "setup": "node setup/setupMessage.mjs && yarn && node setup/setup.mjs && yarn start",
        "setup:copy": "rimraf setupCopy && cp ./package.json ./setup/package.json && cp -r ./setup ./setupCopy",
        "setup:test": "yarn setup:copy && node setup/setupMessage.mjs && yarn && yarn setup:test:init && yarn start",
        "setup:test:init": "cross-env MODE=test node setup/setup.mjs",
        "develop": "GATSBY_ACTIVE_ENV=development gatsby develop -o",
        "start": "yarn develop",
        "build": "GATSBY_ACTIVE_ENV=production gatsby build",
        "build:staging": "GATSBY_ACTIVE_ENV=staging gatsby build",
        "start:mobile": "yarn develop -H 0.0.0.0",
        "serve": "gatsby serve -o",
        "serveMobile": "gatsby serve -o -H 0.0.0.0",
        "optim-img": "node scripts/imageOptimizer.js",
        "https": "http-server public -o -g --ssl --cert ~/.localhost-ssl/test.crt --key ~/.localhost-ssl/test.key",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
        "lint": "eslint --ext .jsx,.js,.cjs,.mjs .",
        "pretty": "prettier --write 'src/**/*.js'",
        "clean": "gatsby clean",
        "stylelint": "stylelint \"src/**/*.styl\" --fix",
        "mrc": "node ./node_modules/mk-react-comp/lib/index.js",
        "semantic-release": "semantic-release",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JimmyBeldone/gatsby-starter-lemonade"
    },
    "bugs": {
        "url": "https://github.com/JimmyBeldone/gatsby-starter-lemonade/issues"
    },
    "dependencies": {
        "@reach/router": "1.3.4",
        "@reduxjs/toolkit": "^1.9.7",
        "clsx": "2.0.0",
        "dotenv": "16.3.1",
        "framer-motion": "^10.16.5",
        "gatsby": "5.12.9",
        "gatsby-plugin-alias-imports": "1.0.5",
        "gatsby-plugin-breakpoints": "1.3.11",
        "gatsby-plugin-image": "3.12.3",
        "gatsby-plugin-manifest": "5.12.3",
        "gatsby-plugin-netlify": "5.1.1",
        "gatsby-plugin-offline": "6.12.3",
        "gatsby-plugin-react-helmet-async": "1.2.3",
        "gatsby-plugin-robots-txt": "1.8.0",
        "gatsby-plugin-sharp": "^5.12.3",
        "gatsby-plugin-sitemap": "6.12.3",
        "gatsby-source-filesystem": "5.12.1",
        "gatsby-transformer-sharp": "5.12.3",
        "lodash.kebabcase": "4.1.1",
        "path-browserify": "1.0.1",
        "process": "0.11.10",
        "prop-types": "15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-headroom": "3.2.1",
        "react-helmet-async": "1.3.0",
        "react-redux": "^8.1.3",
        "reactotron-core-client": "2.8.10",
        "redux": "^4.2.1",
        "redux-persist": "^6.0.0"
    },
    "devDependencies": {
        "@babel/core": "7.23.3",
        "@babel/eslint-parser": "7.23.3",
        "@babel/preset-env": "7.23.3",
        "@babel/preset-react": "7.23.3",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "babel-eslint": "10.1.0",
        "babel-preset-gatsby": "^3.12.1",
        "commitizen": "4.3.0",
        "cross-env": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "devmoji": "2.3.0",
        "eslint": "8.53.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-sort-destructure-keys": "1.5.0",
        "eslint-plugin-sort-keys-fix": "1.1.2",
        "eslint-webpack-plugin": "^4.0.1",
        "gatsby-plugin-eslint": "4.0.4",
        "gatsby-plugin-stylus-resources": "2.0.7",
        "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.32",
        "husky": "8.0.3",
        "lint-staged": "15.1.0",
        "prettier": "3.1.0",
        "reactotron-react-js": "3.3.8",
        "reactotron-redux": "^3.1.3",
        "replace": "^1.2.2",
        "rimraf": "5.0.5",
        "semantic-release": "22.0.7",
        "stylelint": "^15.11.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-prettier": "^4.0.2",
        "stylelint-stylus": "^0.18.0"
    },
    "lint-staged": {
        "{src}/**/*.(js|jsx)": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{css,styl}": "stylelint --fix"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
